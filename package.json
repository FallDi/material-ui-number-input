{
  "name": "material-ui-number-input",
  "version": "5.0.8",
  "description": "The better TextField for number inputs.",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typings": "typings install",
    "start": "webpack && mv ./demo/index.html ./ && node server.js",
    "prepublish": "tsc && npm run prepublish:sed && npm run prepublish:dts",
    "prepublish:sed": "npm run prepublish:sed:typings",
    "prepublish:sed:typings": "sed -i '/\\/\\/\\/ <reference path=\".\\/..\\/typings\\/index.d.ts\" \\/>/d' ./index.js",
    "prepublish:dts": "echo  \"import NumberInput from './NumberInput';\nexport * from './NumberInput';\nexport default NumberInput;\" > ./index.d.ts",
    "postpublish": "rm ./index* ./NumberInput*"
  },
  "keywords": [
    "react",
    "material-ui",
    "input",
    "number",
    "ux",
    "material-design",
    "input-number",
    "user-expects"
  ],
  "author": "Ivo Stratev",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NoHomey/material-ui-number-input.git"
  },
  "peerDependencies": {
    "material-ui": "^0.15.4",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-tap-event-plugin": "^1.0.0"
  },
  "devDependencies": {
    "material-ui": "^0.15.4",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-tap-event-plugin": "^1.0.0",
    "express": "^4.14.0",
    "html-webpack-plugin": "^2.22.0",
    "react-hot-loader": "^1.3.0",
    "ts-loader": "^0.8.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "bind-decorator": "^1.0.2",
    "object-assign": "^4.1.0"
  }
}
